doctype html
html
  head
    style.
      body, html {
        height: 100%;
        width: 100%;
      }
      #map {
      width: 98%;
      height: 98%;
      }

  body
    #map
    script(src='https://maps.googleapis.com/maps/api/js?key=GMAPS_KEY_HERE')

    script(src='http://cdn.rawgit.com/HPNeo/gmaps/master/gmaps.js')
    // <script src="https://rawgit.hubusercontent.com/HPNeo/gmaps/master/gmaps.js" type="text/javascript"></script>
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js')
    // Google Maps JS API
    // GMaps Library
    script.
      var map;
      var coords = JSON.parse('!{coords}');
      function initMap() {
      map = new google.maps.Map(document.getElementById('map'), {
      center: {lat: coords[0].lat, lng: coords[0].lon},
      zoom: 8
      });
      }
      /* Map Object */
      var mapObj = new GMaps({
      el: '#map',
      lat: coords[0].lat,
      lng: coords[0].lon
      });
      // console.log(mapcoords);
      var mapcoords = [];
      for(var i=0; i<coords.length;i++ ){
      mapcoords.push([coords[i].lat,coords[i].lon]);
      }
      console.log(mapcoords);
      var path = mapcoords;
      var pl = mapObj.drawPolyline({
      path: path,
      strokeColor: '#777777',
      strokeOpacity: 1,
      strokeWeight: 10
      });
